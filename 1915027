22 Program to find transpose of a matrix
#include <stdio.h>
int main()
{
	int a[3][3],b[3][3],c[3][3];
	printf("Enter the value for matrix\n");
	for(int i=1;i<=3;i++)
	{
		for(int j=1;j<=3;j++)
		scanf("%d",&a[i][j]);
	}
	for(int i=1;i<=3;i++)
	{
		for(int j=1;j<=3;j++)
		c[j][i]=a[i][j];
	}
	printf("Matrix\n");
	for(int i=1;i<=3;i++)
	{
		for(int j=1;j<=3;j++)
		printf("%d\t",a[i][j]);
		printf("\n");
	}
	printf("Result of Transpose of Matrix\n");
	for(int i=1;i<=3;i++)
	{
		for(int j=1;j<=3;j++)
		printf("%d\t",c[i][j]);
		printf("\n");
	}
}
output
Enter the value for matrix
11
22
3
4
55
66
7
8
9
Matrix
11 22 3
4 55 66
7 8 9
Result of Transpose of Matrix
11 4 7
22 55 8
3 66 9

23 Program to multiply matrices
#include <stdio.h>
int main()
{
	int a[3][3],b[3][3],c[3][3],sum;
	printf("Enter the value for first matrix\n");
	for(int i=1;i<=3;i++)
	{
		for(int j=1;j<=3;j++)
		scanf("%d",&a[i][j]);
	}
	printf("Enter the value for second matrix\n");
	for(int i=1;i<=3;i++)
	{
		for(int j=1;j<=3;j++)
		scanf("%d",&b[i][j]);
	}
	for(int i=1;i<=3;i++)
	{
		for(int j=1;j<=3;j++)
		{
			sum=0;
			for(int k=1;k<=3;k++)
			sum=sum+a[i][k]*b[k][j];
			c[i][j]=sum;
		}
	}
	printf("First Matrix\n");
	for(int i=1;i<=3;i++)
	{
		for(int j=1;j<=3;j++)
		printf("%d\t",a[i][j]);
		printf("\n");
	}
	printf("Second Matrix\n");
	for(int i=1;i<=3;i++)
	{
		for(int j=1;j<=3;j++)
		printf("%d\t",b[i][j]);
		printf("\n");
	}
	printf("Result of Multiplication of Two Matrix\n");
	for(int i=1;i<=3;i++)
	{
		for(int j=1;j<=3;j++)
		printf("%d\t",c[i][j]);
		printf("\n");
	}
}
output
after entering values answer is
First Matrix
1 1 1
1 11 1
1 1 11
Second Matrix
0 11 11
0 1 1
1 1 1
Result of Multiplication of Two
Matrix
1 13 13
1 23 23
11 23 23

24 Program to find square of a number using function
#include<stdio.h>
int square(int x);
int main()
{
	int n,s;
	printf("Enter the number: ");
	scanf("%d",&n);
	s=square(n);
	printf("Square of %d= %d\n",n,s);
}
int square(int x)
{
	int s=x*x;
	return s;
}
output
Enter the number: 7
Square of 7= 49

25 Program to swap a number using call by value
#include <stdio.h>
void swap(int a, intb);
int main()
{
	int x, y;
	printf("Enter the value of x and y\n");
	scanf("%d%d",&x,&y);
	printf("Before Swapping\nx = %d\ny = %d\n", x, y);
	swap(x, y); 
	printf("After Swapping\nx = %d\ny = %d\n", x, y);
	return 0;
}
void swap(int a, int b)
{
	int temp;
	temp = b;
	b = a;
	a = temp;
} 
output
enter the value of x and y
33
2
Before Swapping
x = 33
y = 2
After Swapping
x = 33
y = 2

26 Program to swap numbers using call by reference
#include <stdio.h>
void swap(int * num1, int * num2);
	int main()
{
	int num1, num2;
	printf("Enter two numbers: ");
	scanf("%d%d", &num1, &num2);
	printf("Before swapping in main n");
	printf("Value of num1 = %d \n", num1);
	printf("Value of num2 = %d \n\n", num2);
	swap(&num1, &num2);
	printf("After swapping in main n");
	printf("Value of num1 = %d \n", num1);
	printf("Value of num2 = %d \n\n", num2);
	return 0;
}
void swap(int * num1, int * num2)
{
	int temp;
	temp = *num1;
	*num1= *num2;
	*num2= temp;
} 
output
enter two numbers: 33
22
Before swapping in main nValue o
f num1 = 33
Value of num2 = 22

After swapping in main nValue of
num1 = 22
Value of num2 = 33

27 Program to find factorial using recursion
#include <stdio.h>
int factorial(int n);
int main()
{
	int n;
	printf("Enter the number: ");
	scanf("%d", &n);
	printf("Factorial of %d = %d\n", n,factorial(n));
	return 0;
}
int factorial(int n)
{
	if (n>=1)
	return n*factorial(n-1);
	else
	return 1;
} 
output
enter the number: 5
Factorial of 5 = 120

28 Program to find fibonacci series using recursion
#include<stdio.h>
int Fibonacci(int);
int main()
{
	int n,i=0;
	printf("Enter the limit: ");
	scanf("%d",&n);
	printf("Fibonacci series\n");
	for(int j=0;j<=n;j++)
	{
		printf("%d ",Fibonacci(i));
		i++; 
	}
	printf("\n");
	return 0;
}
int Fibonacci(int n)
{
	if(n==0)
	return 0;
	else if(n==1)
	return 1;
	else
	return ( Fibonacci(n-1) + Fibonacci(n-2) );
}  
output
enter the limit: 5
Fibonacci series
011235

29 Program to show elements in structure
#include<stdio.h>
struct doctor
{ char name[20];
float age;
int gender;
char disease[20];
};
int main()
{ struct doctor patient;
 printf("enter the information:");
 printf("name  :\n");
scanf("%s",patient.name);
printf("age:\n");
scanf("%f",&patient.age);
printf("gender:\n");
scanf("%d",&patient.gender);
printf("disease:\n");
scanf(" %s",patient.disease);
printf("A patient %s of age %.1f and gender %d has %s disease.\n",patient.name,\
                       patient.age,patient.gender,patient.disease); 
}

output
enter the information:name :
abc
age:
12.1
disease:
asthma
gender:
1
A patient abc of age 12.1 and gender 1 has asthma disease.

30 Program of array of structures
#include<stdio.h>
struct student
{
    int rollno;
    char name[20];
};
int main()
{
    int i;
    struct student st[3];
    printf("enter elements\n");
    for(i=0;i<3;i++)
    {
        printf("rollno\n");
        scanf("%d",&st[i].rollno);
        printf("name\n");
        scanf("%s",st[i].name);
    }
    for(i=0;i<3;i++)
    printf("roll no. is %d of %s\n",st[i].rollno,st[i].name);
}
output
enter elements
rollno
1
name
abc
rollno
2
name
pqr
rollno
3
name
xyz
roll no. is 1 of abc
roll no. is 2 of pqr

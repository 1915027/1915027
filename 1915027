{
int a[10];
enterarray(a,10);
printarray(a,10);
return 0;
};
void enterarray(int a[],int b)
{
int i=0;
printf("enter elements:\n");
for(i=0;i<b;i++)
{printf("enter a[%d]=",i);
scanf("%d",&a[i]);}
printf("\n");}
void printarray(int a[],int b)
{
int i=0;
printf("elements are:\n");
for(i=0;i<b;i++)
printf("a[%d]=%d\n",i,a[i]);
}


9 Program to find reverse of a number
#include<stdio.h>
int main()
{
int a,n;
int r=0;
printf("enter the number to get its reverse:");
scanf("%d",&n);
while(n>=1)
{ a=n%10;
n=n/10;
r=r*10+a;
}
printf("the reverse is:%d",r);
return 0;
}
output
enter the number to get its reverse:321
the reverse is:123

10 Program to find fizzbuzz
#include<stdio.h>
int main()
{ int i; 
    for (i=1; i<=100; i++) 
    { 
        
        if (i%15 == 0)         
            printf ("FizzBuzz\t");     
        else
        if ((i%3) == 0)     
            printf("Fizz\t");                 
        else 
        if ((i%5) == 0)                        
            printf("Buzz\t");                  
        else           
            printf("%d\t", i);                  
  
    } 
  
    return 0; 
    }
    

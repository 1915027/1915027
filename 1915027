19 Program to enter and print elements in a two dimensional array
#include<stdio.h>
int main()
{
	int a[3][3];
	printf("Enter the value for 3*3 matrix\n");
	for(int i=1;i<=3;i++)
	{
		for(int j=1;j<=3;j++)
		scanf("%d",&a[i][j]);
	}
	printf("Matrix A\n");
	for(int i=1;i<=3;i++) 
	{
		for(int j=1;j<=3;j++)
		printf("%d\t",a[i][j]);
		printf("\n");
	}
}
output

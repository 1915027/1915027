14Program to find that a number is prime or not
#include <stdio.h>
int main()
{
   int i, num, p = 0;
   printf("Please enter a number: \n");
   scanf("%d", &num);
   for(i=1; i<=num; i++)
   {
      if(num%i==0)
      {
         p++;
      }
   }   }

   if(p==2)
   {
      printf("Entered number is %d "\
             "and it is a prime number.",num);
   }
   else
   {
      printf("Entered number is %d "\
             "and it is not a prime number.",num);
   }
}

output
Please enter a number:
12
Entered number is 12 and it is not a prime number.

15 Program to find a number is palindrome or not
#include<stdio.h>
int main()
{
int a,n,r=0,t;
printf("enter the number:");
scanf("%d",&n);
t=n;
while(n!=0)
{ a=n%10;
  r=r*10+a;
  n=n/10;
  
  }
if (r==t)
printf("number is palindrome\n");
else
printf("number is not palindrome\n");
return 0;
}
output
enter the number:20502
number is palindrome

16 Program to check that a string is palindrome or not
#include <stdio.h>
#include <string.h>
 
int main()
{
    char a[1000];
    int i,n,c=0;
 
    printf("Enter  the string : ");
    scanf("%s",a);
    n=strlen(a);
 
    for(i=0;i<n/2;i++)  
    {
    	if(a[i]==a[n-i-1])
    	c++;
 
 	}
 	if(c==i)
 	    printf("string is palindrome");
    else
        printf("string is not palindrome");
 
 	 
     
    return 0;
}
output
Enter the string : madam
string is palindrome

17 Program to find fibonacci series
#include <stdio.h>
int main()
{
	int n,a=0,b=1,c=0,i;
	printf("Enter ther limit of series ");
	scanf("%d",&n);
	printf("%d %d ",a,b);
	for(i=2;i<n;i++)
	{
		c=a+b;
		printf("%d ",c);
		a=b;
		b=c;
	}
	printf("\n");
	return 0;
}
output
Enter ther limit of series 6
0 1 1 2 3 5

